<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="626.0" prefWidth="303.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.assignment7.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" alignment="TOP_LEFT" text="Loan Amount" VBox.vgrow="SOMETIMES">
      <VBox.margin>
         <Insets />
      </VBox.margin></Label>
   <TextField fx:id="Loanamm" alignment="TOP_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="122.0" />
   <Label text="Number of Years" />
   <ScrollPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="92.0" prefWidth="56.0">
     <content>
       <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" prefHeight="376.0" prefWidth="42.0">
            <children>
               <RadioButton fx:id="num1" mnemonicParsing="false" text="1">
                  <toggleGroup>
                     <ToggleGroup fx:id="grp" />
                  </toggleGroup></RadioButton>
               <RadioButton fx:id="num2" mnemonicParsing="false" prefHeight="17.0" prefWidth="30.0" text="2" toggleGroup="$grp" translateY="20.0" />
               <RadioButton fx:id="num3" mnemonicParsing="false" prefHeight="18.0" prefWidth="30.0" text="3" toggleGroup="$grp" translateY="40.0" />
               <RadioButton fx:id="num4" mnemonicParsing="false" prefHeight="17.0" prefWidth="30.0" text="4" toggleGroup="$grp" translateY="60.0" />
               <RadioButton fx:id="num5" mnemonicParsing="false" prefHeight="17.0" prefWidth="30.0" text="5" toggleGroup="$grp" translateY="80.0" />
               <RadioButton fx:id="num6" mnemonicParsing="false" prefHeight="17.0" prefWidth="30.0" text="6" toggleGroup="$grp" translateY="100.0" />
               <RadioButton fx:id="num7" mnemonicParsing="false" prefHeight="18.0" prefWidth="30.0" text="7" toggleGroup="$grp" translateY="120.0" />
               <RadioButton fx:id="num8" mnemonicParsing="false" prefHeight="17.0" prefWidth="30.0" text="8" toggleGroup="$grp" translateY="140.0" />
               <RadioButton fx:id="num9" mnemonicParsing="false" prefHeight="18.0" prefWidth="30.0" text="9" toggleGroup="$grp" translateY="160.0" />
               <RadioButton fx:id="num10" mnemonicParsing="false" prefHeight="18.0" prefWidth="35.0" text="10" toggleGroup="$grp" translateY="180.0" />
               <RadioButton fx:id="num11" mnemonicParsing="false" prefHeight="17.0" prefWidth="35.0" text="11" toggleGroup="$grp" translateY="200.0" />
               <RadioButton fx:id="num12" mnemonicParsing="false" prefHeight="1.0" prefWidth="35.0" text="12" toggleGroup="$grp" translateY="220.0" />
               <RadioButton fx:id="num13" mnemonicParsing="false" prefHeight="0.0" prefWidth="35.0" text="13" toggleGroup="$grp" translateY="240.0" />
               <RadioButton fx:id="num14" mnemonicParsing="false" prefHeight="3.0" prefWidth="35.0" text="14" toggleGroup="$grp" translateY="260.0" />
               <RadioButton fx:id="num15" mnemonicParsing="false" prefHeight="0.0" prefWidth="35.0" text="15" toggleGroup="$grp" translateY="280.0" />
               <RadioButton fx:id="num16" layoutX="-1.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="42.0" text="16" toggleGroup="$grp" translateY="300.0" />
               <RadioButton fx:id="num18" layoutX="5.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="25.0" text="18" toggleGroup="$grp" translateY="320.0" />
               <RadioButton fx:id="num19" mnemonicParsing="false" prefHeight="17.0" prefWidth="42.0" text="19" toggleGroup="$grp" translateY="340.0" />
               <RadioButton fx:id="num20" mnemonicParsing="false" prefHeight="17.0" prefWidth="42.0" text="20" toggleGroup="$grp" translateY="360.0" />
               <RadioButton fx:id="num17" layoutX="3.0" mnemonicParsing="false" prefHeight="9.0" prefWidth="26.0" text="17" toggleGroup="$grp" translateY="380.0" />
            </children>
         </AnchorPane>
     </content>
   </ScrollPane>
   <Label text="Interest Rates" />
   <ComboBox fx:id="interest" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="70.0" />
   <Label text="In Deferment?" />
   <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="22.0" prefWidth="63.0">
      <children>
         <RadioButton fx:id="No" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="22.0" prefWidth="46.0" selected="true" text="No">
            <toggleGroup>
               <ToggleGroup fx:id="grp2" />
            </toggleGroup>
         </RadioButton>
         <RadioButton fx:id="YES" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Yes" toggleGroup="$grp2" translateY="20.0" />
      </children>
   </Pane>
   <Label text="Options" />
   <Pane prefHeight="52.0" prefWidth="263.0">
      <children>
         <CheckBox fx:id="MISS" mnemonicParsing="false" text="No Missing Payments" />
         <CheckBox fx:id="AUTOWITH" layoutY="-3.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="227.0" text="Automatic Withdrawal" translateY="20.0" />
      </children>
   </Pane>
   <Button fx:id="CALC" mnemonicParsing="false" onAction="#calc" text="Calculate" />
   <Label fx:id="AMMOUNT" prefHeight="18.0" prefWidth="177.0" text="0.00">
      <graphic>
         <Label text="Annual Payment:" />
      </graphic>
   </Label>
</VBox>
